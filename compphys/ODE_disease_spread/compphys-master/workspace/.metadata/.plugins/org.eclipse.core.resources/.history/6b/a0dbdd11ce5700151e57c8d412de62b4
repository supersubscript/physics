
public class Disease {
	private double alpha;// = 0.33; // recovery rate
	private double beta; //= 0.8; // infection rate
	private double gamma; //= 0.0002; // vaccination rate

	public Disease(double a, double b, double c) {
		this.alpha = a;
		this.beta = b;
		this.gamma = c; 
	}

	public double getAlpha() {
		return alpha;
	}

	public void setAlpha(double alpha) {
		this.alpha = alpha;
	}

	public double getBeta() {
		return beta;
	}

	public void setBeta(double beta) {
		this.beta = beta;
	}

	public double getGamma() {
		return gamma;
	}

	public void setGamma(double gamma) {
		this.gamma = gamma;
	};
	
	
}
